             />

    </RelativeLayout>

    <ImageView
        android:id="@+id/fulscreenad"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:adjustViewBounds="true"
        android:scaleType="fitXY"
        android:visibility="invisible"
        />

     </RelativeLayout>

