        <RelativeLayout android:layout_width="10dp" android:layout_height="10dp">
            <LinearLayout android:orientation="horizontal" android:id="@+id/thirdOuterCircle" android:background="@drawable/sdk_outer_circle" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_centerInParent="true"/>
        </RelativeLayout>
        <RelativeLayout android:layout_width="10dp" android:layout_height="10dp">
            <LinearLayout android:orientation="horizontal" android:id="@+id/forthOuterCircle" android:background="@drawable/sdk_outer_circle" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_centerInParent="true"/>
        </RelativeLayout>
        <RelativeLayout android:layout_width="10dp" android:layout_height="10dp">
            <LinearLayout android:orientation="horizontal" android:id="@+id/fifthOuterCircle" android:background="@drawable/sdk_outer_circle" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_centerInParent="true"/>
        </RelativeLayout>
    </LinearLayout>
</RelativeLayout>
