<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:emojicon="http://schemas.android.com/apk/res-auto"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@drawable/clearroundedlayout">

    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:padding="5.0dip">


        <RelativeLayout
            android:id="@+id/lytyoutube"
            android:layout_width="fill_parent"
            android:layout_height="50.0dip"
            >

            <ImageView
                android:id="@+id/youtube"
                android:layout_width="40.0dip"
                android:layout_height="40.0dip"
                android:layout_centerVertical="true"
                android:layout_marginLeft="5.0dip"
                android:src="@drawable/new_youtube" />

            <TextView
                android:id="@+id/txtyoutube"
                style="@android:style/TextAppearance.Medium"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="5.0dip"
                android:layout_toRightOf="@+id/youtube"
                android:text="Download From Youtube" />
        </RelativeLayout>

        <View
            android:id="@+id/view1"
            android:layout_width="fill_parent"
            android:layout_height="1.0dip"
            android:layout_below="@+id/lytyoutube"
            android:layout_marginTop="5.0dip"
            android:layout_marginBottom="5.0dip"
            android:background="@android:color/darker_gray" />

        <RelativeLayout
            android:id="@+id/lytfacebook"
            android:layout_width="fill_parent"
            android:layout_height="50.0dip"
            android:layout_below="@+id/view1">

            <ImageView
                android:id="@+id/facebook"
                android:layout_width="40.0dip"
                android:layout_height="40.0dip"
                android:layout_centerVertical="true"
                android:layout_marginLeft="5.0dip"
                android:src="@drawable/new_fblogo" />

            <TextView
                android:id="@+id/txtfacebook"
                style="@android:style/TextAppearance.Medium"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="5.0dip"
                android:layout_toRightOf="@+id/facebook"
                android:text="Download From Facebook" />
        </RelativeLayout>

        <View
            android:id="@+id/view2"
            android:layout_width="fill_parent"
            android:layout_height="1.0dip"
            android:layout_below="@+id/lytfacebook"
            android:layout_marginTop="5.0dip"
            android:layout_marginBottom="5.0dip"
            android:background="@android:color/darker_gray" />


        <RelativeLayout
            android:id="@+id/lytinstagram"
            android:layout_width="fill_parent"
            android:layout_height="50.0dip"
            android:layout_below="@+id/view2">

            <ImageView
                android:id="@+id/instagram"
                android:layout_width="40.0dip"
                android:layout_height="40.0dip"
                android:layout_centerVertical="true"
                android:layout_marginLeft="5.0dip"
                android:src="@drawable/new_instagram" />

            <TextView
                android:id="@+id/txtinstagram"
                style="@android:style/TextAppearance.Medium"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="5.0dip"
                android:layout_toRightOf="@+id/instagram"
                android:text="Download From Instagram" />
        </RelativeLayout>


        <View
            android:id="@+id/view3"
            android:layout_width="fill_parent"
            android:layout_height="1.0dip"
            android:layout_below="@+id/lytinstagram"
            android:layout_marginTop="5.0dip"
            android:layout_marginBottom="5.0dip"
            android:background="@android:color/darker_gray" />

        <RelativeLayout
            android:id="@+id/lyttiktok"
            android:layout_width="fill_parent"
            android:layout_height="50.0dip"
            android:layout_below="@+id/view3"
            >

            <ImageView
                android:id="@+id/tiktok"
                android:layout_width="40.0dip"
                android:layout_height="40.0dip"
                android:layout_centerVertical="true"
                android:layout_marginLeft="5.0dip"
                android:src="@drawable/new_tiktok" />

            <TextView
                android:id="@+id/txttiktok"
                style="@android:style/TextAppearance.Medium"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="5.0dip"
                android:layout_toRightOf="@+id/tiktok"
                android:text="Download From TikTok" />
        </RelativeLayout>

        <View
            android:id="@+id/view4"
            android:layout_width="fill_parent"
            android:layout_height="1.0dip"
            android:layout_below="@+id/lyttiktok"
            android:layout_marginTop="5.0dip"
            android:layout_marginBottom="5.0dip"
            android:background="@android:color/darker_gray" />

        <RelativeLayout
            android:id="@+id/lyttwitter"
            android:layout_width="fill_parent"
            android:layout_height="50.0dip"
            android:layout_below="@+id/view4">

            <ImageView
                android:id="@+id/twitter"
                android:layout_width="40.0dip"
                android:layout_height="40.0dip"
                android:layout_centerVertical="true"
                android:layout_marginLeft="5.0dip"
                android:src="@drawable/new_twitter" />

            <TextView
                android:id="@+id/txttwitter"
                style="@android:style/TextAppearance.Medium"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="5.0dip"
                android:layout_toRightOf="@+id/twitter"
                android:text="Download From Twitter" />
        </RelativeLayout>



        <View
            android:id="@+id/view5"
            android:layout_width="fill_parent"
            android:layout_height="1.0dip"
            android:layout_below="@+id/lyttwitter"
            android:layout_marginTop="5.0dip"
            android:layout_marginBottom="5.0dip"
            android:background="@android:color/darker_gray" />

        <RelativeLayout
            android:id="@+id/lytsharechat"
            android:layout_width="fill_parent"
            android:layout_height="50.0dip"
            android:layout_below="@+id/view5">

            <ImageView
                android:id="@+id/sharechat"
                android:layout_width="40.0dip"
                android:layout_height="40.0dip"
                android:layout_centerVertical="true"
                android:layout_marginLeft="5.0dip"
                android:src="@drawable/new_sharechat" />

            <TextView
                android:id="@+id/txtsharechat"
                style="@android:style/TextAppearance.Medium"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="5.0dip"
                android:layout_toRightOf="@+id/sharechat"
                android:text="Download From ShareChat" />
        </RelativeLayout>

        <View
            android:id="@+id/view6"
            android:layout_width="fill_parent"
            android:layout_height="1.0dip"
            android:layout_below="@+id/lytsharechat"
            android:layout_marginTop="5.0dip"
            android:layout_marginBottom="5.0dip"
            android:background="@android:color/darker_gray" />

        <RelativeLayout
            android:id="@+id/lytpintrest"
            android:layout_width="fill_parent"
            android:layout_height="50.0dip"
            android:layout_below="@+id/view6">

            <ImageView
                android:id="@+id/pintrest"
                android:layout_width="40.0dip"
                android:layout_height="40.0dip"
                android:layout_centerVertical="true"
                android:layout_marginLeft="5.0dip"
                android:src="@drawable/new_pintrest" />

            <TextView
                android:id="@+id/txtpintrest"
                style="@android:style/TextAppearance.Medium"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="5.0dip"
                android:layout_toRightOf="@+id/pintrest"
                android:text="Download From Pinterest" />
        </RelativeLayout>

    </RelativeLayout>
</RelativeLayout>